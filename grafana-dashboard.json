{
  "dashboard": {
    "title": "Test Environment Monitoring",
    "tags": ["test-env", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Flask Requests per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(flask_http_requests_total[1m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Flask Response Time (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(flask_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "PostgreSQL Database Size",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "pg_database_size_bytes{datname=\"testdb\"} / 1024 / 1024",
            "legendFormat": "Database Size",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "mbytes", "label": "Size (MB)"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "PostgreSQL Transactions",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(pg_stat_database_xact_commit[1m])",
            "legendFormat": "Commits",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_xact_rollback[1m])",
            "legendFormat": "Rollbacks",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Transactions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Redis Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "redis_used_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "mbytes", "label": "Memory (MB)"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Redis Keys",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "redis_keyspace_keys",
            "legendFormat": "Keys in {{db}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Keys"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Redis Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(redis_keyspace_hits_total[1m])) / (sum(rate(redis_keyspace_hits_total[1m])) + sum(rate(redis_keyspace_misses_total[1m]))) * 100",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate (%)", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Redis Commands per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[1m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Commands/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "PostgreSQL Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(pg_stat_database_blks_hit[1m])) / (sum(rate(pg_stat_database_blks_hit[1m])) + sum(rate(pg_stat_database_blks_read[1m]))) * 100",
            "legendFormat": "Cache Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate (%)", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Flask Errors (4xx, 5xx)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(flask_http_requests_total{status=~\"4..|5..\"}[1m])) by (status)",
            "legendFormat": "Status {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      }
    ]
  }
}

